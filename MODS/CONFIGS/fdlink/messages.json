{
  "minecraftToDiscord": {
    "playerMessage": {
      "customMessage": "\u003c%player\u003e %message",
      "useCustomMessage": true
    },
    "serverStarting": ":exclamation: **Server is starting!**",
    "serverStarted": ":white_check_mark: **Server Started.**",
    "serverStopping": ":exclamation: **Server is stopping!**",
    "serverStopped": ":octagonal_sign: **Server stopped**",
    "channelDescription": "Playercount : %playercount/%maxplayercount,\n Uptime : %uptime",
    "playerJoined": {
      "customMessage": ":green_circle: **%player** joined the game",
      "useCustomMessage": true
    },
    "playerJoinedRenamed": {
      "customMessage": ":green_circle: **%new (formerly known as %old)** joined the game",
      "useCustomMessage": true
    },
    "playerLeft": {
      "customMessage": ":red_circle: **%player** left the game",
      "useCustomMessage": true
    },
    "advancementTask": {
      "customMessage": ":medal: **%player has made the advancement %advancement**",
      "useCustomMessage": true
    },
    "advancementChallenge": {
      "customMessage": ":medal: **%player has completed the challenge %advancement**",
      "useCustomMessage": true
    },
    "advancementGoal": {
      "customMessage": ":medal: **%player has reached the goal %advancement**",
      "useCustomMessage": true
    },
    "deathMsgPrefix": ":skull: **",
    "deathMsgPostfix": "**",
    "atATellRaw": "%message"
  },
  "discordToMinecraft": {
    "message": "§9[DISCORD] %player: §f%message"
  },
  "discord": {
    "commandPrefix": "!",
    "botActivities": [
      "!commands",
      "%playercount / %maxplayercount",
      "on %ip",
      "%uptime_D day(s), %uptime_H hour(s), %uptime_M minute(s) and %uptime_S second(s)"
    ]
  }
}